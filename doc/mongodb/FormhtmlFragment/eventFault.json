    "fragment" : "<form class=\"sheet_particulars\" style=\"height: auto;margin-top:15px\">\t\n<!--表单最上面的标题-->\t\n</div>\t\n<div class=\"row\" style=\"height: 20px;margin-top: 10px;padding-left:25px;\" id=\"workInform\">\t\n<!--工单详情和工单评价信息-->\t\n<div style=\"width: 50%;float: left\">\t\n<span class=\"blue-block\">\t</span>\t\n<span class=\"mg-l-10\">\t\n&nbsp;&nbsp;工单详情（编号：\n<span id=\"fromSheetId\">\t</span>\t\n）\n</span>\t\n</div>\t\n<div class=\"satisfiedDiv\" style=\"width: 50%;display: none;float: left;display: inline-flex\">\t\n</div>\t\n<!--隐藏的processKey和关联的salId信息-->\t\n<input type=\"hidden\" datatype=\"hidden\" id=\"processKey\" name=\"processKey\">\t\n<input type=\"hidden\" datatype=\"hidden\" id=\"slaId\" name=\"slaId\">\t<input type=\"hidden\" datatype=\"hidden\" id=\"domainId\" name=\"domainId\" value=\"104000\">\n</div>\t\n<!--工单表信息div-->\t\n<div style=\"height: auto;margin-top: 10px;padding-left:25px;\">\t\n<!--申请人信息div-->\t\n<div class=\"row applyUserInfo\" style=\"height: 22%;\">\t\n<div>\t<span class=\"blue-block\">\t</span>\t<span class=\"mg-l-10\">\t申请人信息</span>\t</div>\t\n<div class=\"col-md-4\">\t\n<div class=\"mg-10\">\t\n<span class=\"title_span_width_dialog\" style=\"min-width: 97px\">\t\n<label>\t来源：</label>\t<b>\t*</b>\t\n</span>\t\n<select class=\"select-width\" datatype=\"select\" id=\"source\" name=\"source\" required=\"required\">\t\n<option value=\"1\">\t电话</option>\t\n<option value=\"2\">\t邮件</option>\t\n<option value=\"3\">\t移动端(微信,QQ等)</option>\t\n<option value=\"5\">\t其它</option>\t\n</select>\t\n</div>\t\n</div>\t\n<div class=\"col-md-4\">\t\n<div class=\"mg-10\">\t\n<span class=\"title_span_width_dialog\" style=\"margin-left: -4px\">\t\n<label>\t申报人电话：</label>\t<b>\t*</b>\t\n</span>\t\n<div class=\"relativeParent\">\t\n<input type=\"tel\" datatype=\"text\" name=\"applyMobile\" id=\"applyMobile\" required=\"required\">\t\n<span>\t\n<i class=\"icon icon-conMan\">\t</i>\t\n</span>\t\n</div>\t\n</div>\t\n</div>\t\n<div class=\"col-md-4\">\t\n<div class=\"mg-10\">\t\n<input type=\"hidden\" value=\"\" id=\"applyUserId\" name=\"applyUserId\">\t\n<span class=\"title_span_width_dialog\">\t\n<label>\t申报人姓名：</label>\t<b>\t*</b>\t\n</span>\t\n<input type=\"text\" datatype=\"text\" name=\"applyUser\" id=\"applyUser\" required=\"required\">\t\n</div>\t\n</div>\t\n<div class=\"col-md-4\">\t\n<div class=\"mg-10\">\t\n<span class=\"title_span_width_dialog\" style=\"min-width: 97px\">\t\n<label>\t申报人邮箱：</label>\t\n</span>\t\n<input type=\"email\" datatype=\"text\" name=\"applyEmail\" id=\"applyEmail\">\t\n</div>\t\n</div>\t\n<div class=\"col-md-4\"><div class=\"mg-10\"> <span class=\"title_span_width_dialog\"> <label> 所属域：</label><b>*</b> </span><input type=\"text\" datatype=\"text\" name=\"belongDomains\" id=\"belongDomains\"> </div></div>\t\n<div class=\"col-md-4\">\t\n<div class=\"mg-10\">\t\n<span class=\"title_span_width_dialog\" style=\"min-width: 97px\">\t\n<label>\t客户姓名：</label>\t\n</span>\t\n<input type=\"text\" datatype=\"text\" name=\"clientUser\" id=\"clientUser\">\t\n</div>\t\n</div>\t\n<div class=\"col-md-4\"><div class=\"mg-10\"> <span class=\"title_span_width_dialog\" style=\"min-width:100px;margin-left: -3px;\"> <label> 部门：</label> </span><input type=\"text\" datatype=\"text\" name=\"belongOrganization\" id=\"belongOrganization\"> </div></div> <div class=\"col-md-4\"><div class=\"mg-10\" style=\"margin-left:5px\"> <span class=\"title_span_width_dialog\" style=\"min-width:100px\"> <label> 地址：</label> </span><input type=\"text\" datatype=\"text\" name=\"address\" id=\"address\"> </div></div> \t\n<div class=\"col-md-4\">\t\n<div class=\"mg-10\">\t\n<span class=\"title_span_width_dialog\">\t\n<label>\t预约维修时间：</label>\t\n</span>\t\n<input type=\"text\" datatype=\"date\" name=\"appointmentTime\" id=\"appointmentTime\" readonly>\t\n</div>\t\n</div>\t\n</div>\t\n<!--请求信息div以及动态生成按钮div-->\t\n<div style=\"height: 78%;clear: both\">\t\n<div>\t<span class=\"blue-block\">\t</span>\t<span class=\"mg-l-10\">\t请求信息</span>\t</div>\t\n<div class=\"row mg-10\">\t\n<div class=\"col-md-4\">\t\n<div class=\"mg-10\">\t\n<span class=\"title_span_width_dialog\" style=\"margin-left:-7px\">\t\n<label>\t请求类型：</label>\t<b>\t*</b>\t\n</span>\t\n<select class=\"select-width\" datatype=\"select\" id=\"requestType\" name=\"requestType\" required=\"required\">\t\n<option value=\"1\">\t报障</option>\t\n<option value=\"2\">\t申请</option>\t\n</select>\t\n</div>\t\n</div>\t\n<div class=\"col-md-4\">\t\n<div class=\"mg-10\">\t\n<span class=\"title_span_width_dialog\" style=\"margin-left:-1px\">\t\n<label>\t紧急程度：</label>\t\n</span>\t\n<select class=\"select-width\" datatype=\"select\" id=\"urgency\" name=\"urgency\">\t\n<option value=\"1\">\t紧急</option>\t\n<option value=\"2\">\t高</option>\t\n<option value=\"3\">\t中</option>\t\n<option value=\"4\">\t低</option>\t\n</select>\t\n</div>\t\n</div>\t\n<div class=\"col-md-4\">\t\n<div class=\"mg-10\">\t\n<span class=\"title_span_width_dialog\" style=\"margin-left:6px\">\t\n<label>\t优先级：</label>\t<b>\t*</b>\t\n</span>\t\n<select class=\"select-width\" datatype=\"select\" id=\"priorityId\" name=\"priorityId\">\t</select>\t\n</div>\t\n</div>\t\n<div class=\"col-md-4\">\t\n<div class=\"mg-10\">\t\n<span class=\"title_span_width_dialog\" style=\"margin-left:-7px\">\t\n<label>\t服务目录：</label>\t<b>\t*</b>\t\n</span>\t\n<select class=\"select-width\" datatype=\"select\" id=\"serviceTreeType\" name=\"serviceTreeType\">\t</select>\t\n</div>\t\n</div>\t\n<div class=\"col-md-4\">\t\n<div class=\"mg-10\">\t\n<span class=\"title_span_width_dialog\" style=\"margin-left:-1px\">\t\n<label>\t服务名称：</label>\t<b>\t*</b>\t\n</span>\t\n<select class=\"select-width\" datatype=\"select\" id=\"serviceCatalogId\" name=\"serviceCatalogId\">\t</select>\t\n</div>\t\n</div>\t\n<div class=\"col-md-4\">\t\n<div class=\"mg-10\">\t\n<span class=\"title_span_width_dialog\" style=\"margin-left:6px\">\t\n<label>\t故障类型：</label>\t<b>\t*</b>\t\n</span>\t\n<select class=\"select-width\" datatype=\"select\" id=\"faultType\" name=\"faultType\">\t\n<option value=\"1\">电力</option>\t\n<option value=\"2\">客户</option>\t\n<option value=\"3\">传输</option>\t\n<option value=\"4\">软件</option>\n\t<option value=\"5\">设备</option>\t\n<option value=\"6\">其他</option>\t\n</select>\t\n</div>\t\n</div>\t\n\t\n</div>\t\n<div class=\"row mg-10\">\t\n<div class=\"col-md-4\">\t\n<span class=\"title_span_width_dialog\" style=\"margin-left:-37px\">\t\n<label>\tSLA响应时限(小时)：</label>\t\n</span>\t\n<label name=\"slaWithTime\" datatype=\"text\" id=\"slaWithTime\" style=\"margin-top: 6px\">\t</label>\t\n<input type=\"hidden\">\t\n</div>\t\n<div class=\"col-md-4\">\t\n<span class=\"title_span_width_dialog\" style=\"margin-left:-30px\">\t\n<label>\tSLA解决时限(小时)：</label>\t\n</span>\t\n<label name=\"slaSalveTime\" datatype=\"text\" id=\"slaSalveTime\" style=\"margin-top: 6px\">\t</label>\t\n</div>\t\n</div>\t\n<div class=\"mg-10\">\t\n<span class=\"title_span_width_dialog\" style=\"margin-left:16px\">\t\n<label>\t主题：</label>\t<b>\t*</b>\t\n</span>\t\n<input type=\"text\" datatype=\"text\" id=\"theme\" name=\"theme\" required=\"required\" style=\"width: 83%;height: 25px\">\t\n</div>\t\n<div class=\"mg-10\" style=\"margin-top: 17px\">\t\n<span class=\"title_span_width_dialog\" style=\"margin-left:16px\">\t\n<label>\t关键字：</label>\t\n</span>\t\n<input type=\"text\" name=\"keyWord\" placeholder=\"多个以,号分隔\" id=\"keyWord\" style=\"width: 83%;height: 25px\" datatype=\"text\">\t\n</div>\t\n<div class=\"mg-10\" style=\"margin-top: 19px\">\t\n<span class=\"title_span_width_dialog\" style=\"margin-left:16px\">\t\n<label>\t描述：</label>\t<b>\t*</b>\t\n</span>\t\n<textarea name=\"description\" datatype=\"text\" id=\"description\" style=\"width: 83%;height: 50px\"></textarea>\t\n</div>\t\n<p style=\"margin-left: 120px\">描述不超过500字</p>\t\n<div class=\"mg-10\" style=\"margin-top: 10px\">\t\n<span class=\"title_span_width_dialog\" style=\"margin-left:16px\">\t\n<label>\t添加附件：</label>\t\n</span>\t\n<input type=\"file\" class=\"form-control\" id=\"upload_file\" name=\"file\" style=\"display: none\">\t\n<button class=\"chooseBtn bth enclosure\" style=\"border:none\" title=\"上传文件\" type=\"button\" onclick=\"file.click();\">\t</button>\t\n<span style=\"position:relative;top:-10px\">\t(支持的文件类型:txt,doc,docx,xls,xlsx,pdf,rar,jpg,png;文件不能大于50M)</span>\t\n<div id=\"files_handle\" style=\"margin-left: 90px;margin-top: 10px\">\t</div>\t\n</div>\t\n<div class=\"clear\" style=\"padding: 10px 0px\">\t\n<span class=\"title_span_width_dialog\" style=\"margin-left:24px\">\t\n<label>\t关联资产：</label>\t\n</span>\t\n<a class=\"icon icon-shadeAdd addSetBtn\">\t</a>\t\n</div>\t\n<div style=\"height: 40%;overflow: auto;width: 92%;padding-left: 110px\">\t\n<table class=\"workConnectAsset\">\t</table>\t\n</div>\t\n<div style=\"margin-top: 17px\" id=\"action_workbench\">\t\n<button class=\"btn bg-dark btn-flat pull-right btn-bottom\" id=\"cancel-btn\" type=\"button\" style=\"margin-right: 8%\">\t取消</button>\t\n</div>\t\n</div>\t\n</div>\t</form>",
